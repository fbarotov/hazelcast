{
  "instance": {
    "hazelcast": {
      "device": {
        "device-name": "tiered_store_device_name",
        "base-dir": "tiered_store_base_dir",
        "block-size": 8192,
        "read-io-thread-count": 1,
        "write-io-thread-count": 1
      },
      "map": {
        "my-map-0": {
          "tiered-store": {
            "enabled": true
          }
        },
        "my-map-1": {
          "tiered-store": {
            "enabled": true,
            "memory-tier": {
              "capacity": "2 GB"
            }
          }
        },
        "my-map-2": {
          "tiered-store": {
            "enabled": true,
            "disk-tier": {
              "enabled": true
            }
          }
        },
        "my-map-3": {
          "tiered-store": {
            "enabled": true,
            "memory-tier": {
              "capacity": "8192 KB"
            },
            "disk-tier": {
              "enabled": true
            }
          }
        }
      }
    }
  },
  "error": null
}
